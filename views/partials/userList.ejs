<head>
    <link rel="stylesheet" href="styles/site.css" />
</head>

<h2 class="sectTitle">Active User List</h2>

<div>  
    <table id="table1">
        <tr>
            <th>Username</th>
            <th>Email</th>
        </tr>
        <tbody>
            <% users.forEach(function(user){ %>
        <tr>
            <td>
                <%= user.username %>
            </td>
            <td>
                <%= user.email %>
            </td>
        </tr>
        </tbody>
        <% }) %>

    </table>      
</div>
